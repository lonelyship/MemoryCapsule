<resources>


    <!-- Base application theme. -->
    <style name="AppTheme2" parent="Theme.AppCompat.Light.DarkActionBar"/>

    <!-- Customize your theme here. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar" >
        <item name="android:windowTranslucentStatus">true</item>
    </style>


    <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@drawable/ui_color_Transparent</item>
        <item name="android:windowBackground">@drawable/ui_color_Transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>


    <color name="colorBlue">#2196F3</color>
    <color name="colorBlueDark">#1976D2</color>
    <color name="colorRed">#FF4081</color>
    <color name="colorPink">#F3B9CD</color>
    <color name="colorWhite">#EEE</color>
    <color name="colorGray">#CCC</color>
    <color name="colorDarkGray">#AAA</color>
    <color name="colorGreenDark">#409999</color>

</resources>
